  "main": "index.js",
  "scripts": {
    "start": "webpack serve --host 0.0.0.0",
    "prebuild": "npm run cleanup",
    "cleanup": "rm -rf build; npm run messages",
    "build": "webpack --mode production",
    "messages:extract": "formatjs extract \"src/**/*.{tsx,ts}\" --out-file generated/messages.json --ignore \"src/**/*.d.{tsx,ts}\"",
    "messages:compile": "formatjs compile generated/messages.json --out-file src/localization/en.json",
    "generate-graphql-types": "dotenv -- graphql-codegen --config codegen.yml",
    "messages": "npm run messages:extract; npm run  messages:compile",
    "deploy": "npm run build && npx serverless deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
